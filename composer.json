{
    "name": "symfony2-magic-route-loader",
    "description": "Loader component for Symfony2 task",
    "license": "proprietary",
    "type": "library",
    "version": "0.1",
    "require": {
        "php": "~7.0",
        "symfony/symfony": "~3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.5"
    },
    "autoload": {
        "psr-4": {
            "TestApp\\": "src/",
            "TestApp\\Tests\\": "tests/"
        }
    },
    "exclude-from-classmap": [
        "tests/"
    ]
}
